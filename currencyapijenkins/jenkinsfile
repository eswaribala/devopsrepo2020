#!groovy
pipeline{
 agent any
 
  tools {
    maven 'maven'
  }
 
 stages{
 
   stage("build"){
   
       steps{
       
          bat "mvn -f ./currencyapijenkins clean install -DskipTests"
          bat "docker build -f currencyapijenkins/dockerfile -t currencyapijenkinsv1 ./currencyapijenkins"
       
       }
   
   }
 
 }
 
 
 
}
