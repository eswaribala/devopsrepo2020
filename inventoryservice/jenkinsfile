#!groovy
pipeline{
 agent any
 
  tools {
    maven 'maven'
  }
 
 stages{
 
   stage("build"){
   
       steps{
       
          bat "mvn -f ./inventoryservice clean install -DskipTests"
          bat "docker build -f inventoryservice/dockerfile -t inventoryserviceapp ./inventoryservice"
                          
       }
   
   }
 
 }
 
 
 
}
